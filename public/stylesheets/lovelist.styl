@font-face
   font-family "Lucida Grande"
   font-style normal
   src url(LucidaGrande.ttf)

body
  font 14px "Lucida Grande", Helvetica, Arial, sans-serif
  text-align center
  color white

.header
  background-color #46ccc4
  width 390px
  text-align center
  padding 15px
  padding-top 7px
  padding-bottom 7px
  margin auto
  height 40px

.left
  float left
  text-align left
  display inline
  
.mid
  text-align center
  display inline
  left 0
  right 0
  margin 0 auto
  position absolute

.logos
  height 30px

.right
  float right
  text-align right
  display inline

.fa-search
  margin-right 10px

.bheader
  height 50px
  width 400px
  text-align left
  padding 10px
  background-color #f0f0f0
  margin auto

.lovelist
  font-size 20px
  color black
  font-weight bold

.garis
  //border-style solid
  border none
  height 1px
  background-color #CFD8EA
  float none
  margin 0
  margin-top 10px

.kiri
  float left
  margin-right 10px

.gambar-barang
  display inline
  height 65px
  width 65px
  
.list-item
  background-color white
  width 420px
  text-align left
  margin auto

.kanan
  margin auto
  margin-left 70px
  display block

.nama-barang
  font-weight bold
  color black
  display block
  
.harga-barang
  color #46ccc4
  display block
  font-size 12px
  
.asd
  margin-top 10px
  //float left
  
.comment
  display inline
  margin-right 10px
  font-size 12px
  
.love
  display inline
  font-size 12px
  
.fa-comment-o, .fa-heart-o
  color #CFD8EA
  margin-right 5px
  
.total-comment, .total-love
  color #CFD8EA
  
.hitam
  color black

.item
  padding 10px
  padding-right 0px
  padding-left 0px
  border-style solid
  border-width 0px
  border-bottom-width 1px
  color #f0f0f0
  
.load
  text-align center
  background-color #f0f0f0
  padding 20px
  margin auto
  width 380px
  
.load-more
  width 100px
  height 30px
  background-color white
  border-style solid
  border-radius 15px
  border 10px #f0f0f0
  font-size 12px
  font-weight bold
  margin-top 10px
  color #46ccc4
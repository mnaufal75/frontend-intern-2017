@font-face
   font-family "Lucida Grande"
   font-style normal
   src url(LucidaGrande.ttf)

body
  font 14px "Lucida Grande", Helvetica, Arial, sans-serif
  text-align center
  color white

.header
  background-color #46ccc4
  width 390px
  text-align center
  padding 15px
  padding-top 7px
  padding-bottom 7px
  margin auto

.left
  float left
  text-align left
  display inline
  
.mid
  text-align center
  display inline

.logos
  height 30px

.right
  float right
  text-align right
  display inline

.fa-search
  margin-right 10px

.bheader
  height 50px
  width 400px
  text-align left
  padding 10px
  background-color #CFD8EA
  margin auto

.username
  font-size 20px
  color black

.garis
  border-style solid
  color #CFD8EA
  float none
  margin 0

.kiri
  float left
  margin-right 10px

.gambar-barang
  display inline
  height 65px
  width 65px
  
.list-item
  background-color white
  width 420px
  text-align left
  margin auto

.kanan
  margin auto
  margin-left 70px
  display block

.nama-barang
  font-weight bold
  color black
  display block
  
.harga-barang
  color #46ccc4
  display block
  font-size 12px
  
.asd
  margin-top 10px
  //float left
  
.comment
  display inline
  margin-right 10px
  font-size 12px
  
.love
  display inline
  font-size 12px
  
.fa-comment-o, .fa-heart-o
  color #CFD8EA
  margin-right 5px
  
.total-comment, .total-love
  color #CFD8EA
  
.hitam
  color black

.item
  margin-bottom 10px
  padding 10px
  
.load
  text-align center
  background-color #F0F0F0
  padding 20px
  margin auto
  width 400px
  
.load-more
  width 100px
  height 30px
  background-color white
  border-style solid
  border-radius 15px
  border 1px
  font-size 12px
  font-weight bold
  margin-top 10px
  color #46ccc4